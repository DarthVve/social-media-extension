{
  "manifest_version": 2,
  "name": "Zwilt",
  "description": "Zwilt is a free service that lets you grow your followers and connections on social media faster over time. We interact with accounts likely to organically grow your own by simulating human behavior in your browser.",
  "version": "14.3.6",
  "icons": {
    "512": "public/images/icon.png"
  },
  "browser_action": {
    "default_icon": "public/images/icon.png",
    "default_popup": "pages/bot.html"
  },
  "content_security_policy": "script-src 'self';  object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*.instagram.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "service-workers/content.js"
      ]
    },
    {
      "matches": [
        "*://*.tinder.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/tinder.js"
      ]
    },
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/linkedin.js"
      ]
    },
    {
      "matches": [
        "*://*.pinterest.com/*",
        "*://*.pinterest.co.uk/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/pinterest.js"
      ]
    },
    {
      "matches": [
        "*://*.tiktok.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/content/tiktok.js"
      ]
    },
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/content/facebook.js"
      ]
    },
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "controllers/twitter.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "libs/jquery-3.5.1.min.js",
      "service-workers/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*.instagram.com/*",
    "storage",
    "webRequest",
    "downloads",
    "https://instoo.com/"
  ]
}